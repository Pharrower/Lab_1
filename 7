#include <iostream>
#include <list>
#include <string>
using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    list<string> list; //создание списка
    int cnt;
    string word;
    cout << "Пожалуйста, введите кол-во строк: " << endl;
    cin >> cnt;
    cout << "Пожалуйста, введите строки: " << endl;
    for (int i = 0; i < cnt; i++) {
        cin >> word;
        list.push_back(word);
    }
    cout << "Строки, начинающиеся с заглавной буквы: " << endl;
    //перебор в циклке
    for (auto iter = list.begin(); iter != list.end(); iter++) {
        if ((*iter)[0] >= 'A' && (*iter)[0] <= 'Z') {
            cout << *iter << endl;
        }
    }
    return 0;
}
