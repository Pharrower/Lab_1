#include <iostream>
#include <list>
using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    list<double> numbers; // Создание списка
    int cnt; // Переменная для кол-ва элементов списка
    double num; //Переменная для элементов списка
    cout << "Пожалуйтса, введите кол-во элементов списка: "; cin >> cnt;
    cout << "Пожалуйтса, введите элементы списка: ";
    for (int i = 0; i < cnt; i++) {
        cin >> num;
        numbers.push_back(num); //добавление элементов в список
    }

    cout << "Список до изменений: " << endl;
    for (double n : numbers) cout << n << " ";
    cout << endl;

    for (auto iter = numbers.begin(); iter != numbers.end(); ++iter) {
        if (*iter >= 10 && *iter <= 20) {
            if (iter != numbers.begin()) {
                iter = numbers.erase(prev(iter));
            }
        }
    }

    cout << "Список после изменений: " << endl;
    for (double n : numbers) cout << n << " ";

    return 0;
}
