#include <iostream>
#include <string>
#include <queue> 
using namespace std;

int main() {
    setlocale(LC_ALL, "rus");
    queue <double> q;  // создали очередь q
    int n, m;
    double a;
    double pr = 1.0;
    cout << "Введите кол-во значений: ";
    cin >> n;
    cout << "Введите сами значения: " << endl;
    for (int i = 0; i < n; i++) {
        cin >> a;
        q.push(a);  // добавляем в очередь элементы
        if (i < (n - 3)) pr *= a;
    }

    // удаляем 3 элемента из очереди
    for (int i = 0; i < 3; i++)
        q.pop();
    cout << "Удалено 3 первых элемента" << endl;

    cout << "Введите кол-во новых значений: ";
    cin >> m;
    cout << "Введите новые значения: " << endl;
    for (int i = 0; i < m; i++) {
        cin >> a;
        q.push(a);  // добавляем в очередь элементы
        pr *= a;
    }

    // выгружаем элементы очереди 
    cout << "Очередь после изменений: " << endl;
    while (!q.empty()) {
        cout << q.front() << "\n";
        q.pop();
    }

    cout << "Произведение элементов очереди: " << pr << endl;
    return 0;
}
